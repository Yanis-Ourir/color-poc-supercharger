{
  "name": "color-hubspot-supercharger",
  "displayName": "Color hubspot supercharger",
  "version": "0.0.1",
  "description": "change your color disposition",
  "author": "yanis",
  "scripts": {
    "dev": "plasmo dev",
    "build": "plasmo build",
    "package": "plasmo package"
  },
  "dependencies": {
    "@plasmohq/storage": "^1.6.0",
    "plasmo": "0.72.2",
    "react": "18.2.0",
    "react-dom": "18.2.0"
  },
  "devDependencies": {
    "@plasmohq/prettier-plugin-sort-imports": "3.6.4",
    "@types/chrome": "0.0.227",
    "@types/node": "18.15.11",
    "@types/react": "18.0.32",
    "@types/react-dom": "18.0.11",
    "prettier": "2.8.7",
    "typescript": "5.0.3"
  },
  "manifest": {
    "host_permissions": [
      "https://*/*",
      "https://app.hubspot.com/reports-dashboard/5483623/view/2593188"
    ],
    "permissions": [
      "activeTab",
      "tabs",
      "scripting"
    ],
    "content_scripts": [
      {
        "matches": [
          "https://*.hubspot.com/*"
        ],
        "js": [
          "../content.tsx"
        ]
      }
    ]
  }
}
